        <link rel="manifest" href="{{ extendURL('/manifest.json')}}">
        <meta name="msapplication-TileColor" content="#ea5455">
        <meta name="msapplication-TileImage" content="/ms-icon-144x144.png">
        <meta name="theme-color" content="#ffffff">

        <script src="{{ extendURL('/js/jquery/jquery.slim.min.js')}}"></script>
        <script src="{{ extendURL('/js/bootstrap/bootstrap.bundle.min.js')}}"></script>
    </head>

    <body>
        {% include "nav.njk" %}

        <main role="main" class="container">
            {% block content %}
            {% endblock %}
        </main>
		<footer>
          {% include "footer.njk" %}
        </footer>
    </body>
</html>
